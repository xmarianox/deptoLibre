        <activity
            android:name=".InmuebleActivity"
            android:label="@string/title_activity_inmueble" >
        </activity>

        <meta-data
            android:name="com.google.android.maps.v2.API_KEY"
            android:value="AIzaSyAuDNuVH9GYUlbh-7VUQBD2UCB_K09vAYs"/>

    </application>

</manifest>
